<struts>
    <!-- 将报错的信息，由英文转变成中文 -->
    <constant name="struts.custom.i18n.resources" value="com.it.action.uploadmessage"></constant>
    <package name="firstTopic" namespace="/" extends="struts-default">
        <action name="firstTopicAction_*" method="{1}" class="com.zdcq.s2sh.action.FirstTopicAction">
            <result name="list">/manage/jsp/firsttopic/list.jsp</result>
            <result name="updateUI">/manage/jsp/firsttopic/edit.jsp</result>
