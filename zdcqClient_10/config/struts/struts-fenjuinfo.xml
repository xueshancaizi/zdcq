    <package name="fenjuinfo" namespace="/" extends="struts-default">
        <action name="FenJuInfoAction_*" method="{1}" class="com.zdcq.s2sh.action.FenJuInfoAction">
            <result name="list">/manage/jsp/fenjuinfo/list.jsp</result>
            <result name="view">/manage/jsp/fenjuinfo/view.jsp</result>
            <result name="edit">/manage/jsp/fenjuinfo/edit.jsp</result>
            
			<result name="addUserUI">/manage/jsp/user/add.jsp</result>
        </action>
    </package>
</struts>
